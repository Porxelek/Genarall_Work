   "source": [
    "number = int(input(\"Please enter an integer: \"))\n",
    "for i in range(2, number) :\n",
    "     if number % i == 0 :\n",
    "        print(\"This is not a prime number.\")\n",
    "        break\n",
    "else:\n",
    "        print(\"This is a prime number.\")"
   ]
